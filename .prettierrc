{
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "importOrder": [
    "^@phork/(.*)$",
    "^types(.*)$",
    "^constants(.*)$",
    "^components(.*)$",
    "^pages(.*)$",
    "^hooks(.*)$",
    "^utils(.*)$",
    "^icons(.*)$",
    "^assets(.*)$",
    "^[./]",
    "^[./]*styles(.*)$"
  ],
  "importOrderSeparation": false,
  "experimentalBabelParserPluginsList" : ["jsx", "typescript"]
}
